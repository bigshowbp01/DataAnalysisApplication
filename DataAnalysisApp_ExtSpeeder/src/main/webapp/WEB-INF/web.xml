<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
    <session-config>
        <session-timeout>20</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>com.company.test.InitServlet</servlet-name>
        <servlet-class>com.company.test.InitServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.company.test.InitServlet</servlet-name>
        <url-pattern>/test/init</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>com.speedment.plugin.extspeeder.runtime.servlet.ExtSpeederShutdownServlet</servlet-name>
        <servlet-class>com.speedment.plugin.extspeeder.runtime.servlet.ExtSpeederShutdownServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.speedment.plugin.extspeeder.runtime.servlet.ExtSpeederShutdownServlet</servlet-name>
        <url-pattern>/test/shutdown</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>com.company.test.test.test.houses.HousesServlet</servlet-name>
        <servlet-class>com.company.test.test.test.houses.HousesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.company.test.test.test.houses.HousesServlet</servlet-name>
        <url-pattern>/test/test/test/houses</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>com.company.test.test.test.people.PeopleServlet</servlet-name>
        <servlet-class>com.company.test.test.test.people.PeopleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.company.test.test.test.people.PeopleServlet</servlet-name>
        <url-pattern>/test/test/test/people</url-pattern>
    </servlet-mapping>
</web-app>
